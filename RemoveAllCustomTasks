@echo off

:: Get a list of all tasks in the Task Scheduler
for /F "tokens=1" %%G in ('schtasks /query /fo csv ^| findstr /v /c:"TaskName"') do (
    :: Remove each task
    schtasks /delete /tn "%%~G" /f > nul
)

:: Display a message indicating the completion of the script
echo All custom tasks have been removed.

:: Pause the script execution to allow time for verification
pause
